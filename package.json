  "keywords": [
    "strix",
    "analytics",
    "game analytics",
    "analysis",
    "game analysis",
    "item system",
    "inventory system",
    "liveops",
    "live-ops",
    "gameops",
    "game-ops"
  ],
  "author": {
    "name": "Strix GameOps",
    "email": "team@strixgameops.com",
    "url": "https://strixgameops.com/"
  },
  "samples": [
        {
            "displayName": "Analytics Example",
