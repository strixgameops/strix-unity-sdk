  "description": "Strix Game Ops plugin for analytics and live-ops",
  "documentationUrl": "https://strixgameops.com/docs/unity/integration",
  "dependencies": {
    "com.unity.purchasing": "4.12.0",
    "com.unity.localization": "1.4.5",
